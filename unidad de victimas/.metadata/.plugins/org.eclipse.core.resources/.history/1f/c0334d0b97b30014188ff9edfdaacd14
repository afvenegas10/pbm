import logging
from datetime import date

logging.basicConfig(
                    level=logging.DEBUG,
                    filename = 'C:/Apache24/logs/UndVictimas/UndVictimas'+str(date.today())+'.log',
                    filemode="w") 

def error(msg):  
    logging.debug(msg)
    print msg

def info(msg):
    logging.error(msg)
    print msg